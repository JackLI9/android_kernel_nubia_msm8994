&soc {
	tlmm: pinctrl@fd510000 {
		compatible = "qcom,msm8994-pinctrl";
		reg = <0xfd510000 0x4000>;
		interrupts = <0 208 0>;
		gpio-controller;
		#gpio-cells = <2>;
		interrupt-controller;
		#interrupt-cells = <2>;

		pmx-uartconsole {
			uart_console_active: uart_console_active {
				mux {
					pins = "gpio4", "gpio5";
					function = "blsp_uart2";
				};

				config {
					pins = "gpio4", "gpio5";
					drive-strength = <2>;
					bias-disable;
				};
			};

			uart_console_sleep: uart_console_sleep {
				mux {
					pins = "gpio4", "gpio5";
					function = "blsp_uart2";
				};

				config {
					pins = "gpio4", "gpio5";
					drive-strength = <2>;
					bias-pull-down;
				};
			};
		};

		// add by ztemt start
		// add the uart pinctrl for IR-remote control
		blsp_uart1_active: uart1_default {
			mux {
				pins = "gpio0", "gpio1", "gpio2", "gpio3";
				function = "blsp_uart1";
			};

			config {
				pins = "gpio0", "gpio1", "gpio2", "gpio3";
				drive-strength = <16>;
				bias-disable;
			};
		};

		blsp_uart1_sleep: uart1_sleep {
			mux {
				pins = "gpio0", "gpio1";
				function = "gpio";
			};

			config {
				pins = "gpio0", "gpio1";
				drive-strength = <2>;
				bias-pull-down;
				output-low;
			};
		};
		// add by ztemt end

		blsp_uart2_active: uart2_default {
			mux {
				pins = "gpio45", "gpio46", "gpio47", "gpio48";
				function = "blsp_uart8";
			};

			config {
				pins = "gpio45", "gpio46", "gpio47", "gpio48";
				drive-strength = <16>;
				bias-disable;
			};
		};

		blsp_uart2_sleep: uart2_sleep {
			mux {
				pins = "gpio45", "gpio46", "gpio47", "gpio48";
				function = "gpio";
			};

			config {
				pins = "gpio45", "gpio46", "gpio47", "gpio48";
				drive-strength = <2>;
				bias-disable;
			};
		};

		pmx_mdss: pmx_mdss {
			// label = "mdss-pins";
			// qcom,pin-func = <0>;
			mdss_dsi_active: active {
				config {
					drive-strength = <8>; /* 8 mA */
					bias-disable = <0>; /* no pull */
				};
			};
			mdss_dsi_suspend: suspend {
				config {
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
				};
			};
		};

		pmx_mdss_te: pmx_mdss_te {
			// label = "mdss-te-pins";
			// qcom,pin-func = <1>;
			mdss_te_active: active {
				config {
					drive-strength = <2>; /* 8 mA */
					bias-pull-down = <0>; /* pull down*/
					input-debounce = <0>;
				};
			};
			mdss_te_suspend: suspend {
				config {
					drive-strength = <2>; /* 2 mA */
					bias-pull-down; /* pull down */
					input-debounce = <0>;
				};
			};
		};

		pmx_hdmi_cec: pmx_hdmi_cec {
			// qcom,pin-func = <1>;
			// label = "hdmi-cec-pins";
			mdss_hdmi_cec_active: cec_active {
				config {
					drive-strength = <2>;
					bias-pull-up;
				};
			};
			mdss_hdmi_cec_suspend: cec_suspend {
				config {
					drive-strength = <2>;
					bias-pull-down;
				};
			};
		};

		pmx_hdmi_ddc: pmx_hdmi_ddc {
			// qcom,pin-func = <1>;
			// label = "hdmi-ddc-pins";
			mdss_hdmi_ddc_active: ddc_active {
				config {
					drive-strength = <2>;
					bias-pull-up;
				};
			};
			mdss_hdmi_ddc_suspend: ddc_suspend {
				config {
					drive-strength = <2>;
					bias-disable;
				};
			};
		};

		pmx_hdmi_hpd: pmx_hdmi_hpd {
			// qcom,pin-func = <1>;
			// label = "hdmi-hpd-pin";
			mdss_hdmi_hpd_active: hpd_active {
				config {
					drive-strength = <16>;
					bias-pull-down;
				};
			};
			mdss_hdmi_hpd_suspend: hpd_suspend {
				config {
					drive-strength = <2>;
					bias-pull-down;
				};
			};
		};

		mhl_intr: mhl_intr {
			// qcom,pins = <&gp 57>;
			// qcom,num-grp-pins = <1>;
			// qcom,pin-func = <0>;
			// label = "mhl_intr";

			mhl_intr_active: mhl_intr_active {
				mux {
					pins = "gpio57";
					function = "gpio";
				};

				config {
					pins = "gpio57";
					bias-pull-up;
					input-enable;
				};
			};
		};

		mhl_reset: mhl_reset {
			// qcom,pins = <&gp 58>;
			// qcom,num-grp-pins = <1>;
			// qcom,pin-func = <0>;
			// label = "mhl_reset";

			mhl_reset_active: mhl_reset_active {
				mux {
					pins = "gpio58";
					function = "gpio";
				};

				config {
					pins = "gpio58";
					drive-strength = <2>;
					bias-pull-down;
					output-low;
				};
			};
		};

		spi0 {

			spi_0_active: spi0_active {
				/* active state */
				mux {
					/* MOSI, MISO, CLK */
					pins = "gpio0", "gpio1", "gpio3";
					function = "blsp_spi1";
				};

				config {
					pins = "gpio0", "gpio1", "gpio3";
					drive-strength = <12>; /* 12 MA */
					bias-disable = <0>; /* No PULL */
				};
			};


			spi_0_sleep: spi0_sleep {
				/* suspended state */
				mux {
					/* MOSI, MISO, CLK */
					pins = "gpio0", "gpio1", "gpio3";
					function = "blsp_spi1";
				};

				config {
					pins = "gpio0", "gpio1", "gpio3";
					drive-strength = <2>; /* 2 MA */
					bias-pull-down; /* PULL Down */
				};
			};

			spi_0_cs1_active: cs1_active {
				/* CS */
				mux {
					pins = "gpio8";
					function = "blsp_spi1";
				};

				config {
					pins = "gpio8";
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};

			spi_0_cs1_sleep: cs1_sleep {
				/* CS */
				mux {
					pins = "gpio8";
					function = "blsp_spi1";
				};

				config {
					pins = "gpio8";
					drive-strength = <2>;
					bias-disable = <0>;
				};
			};
		};

		
	/* spi_10 */
		spi10 {
			spi_10_active: spi10_active {

				mux {
					/* MOSI, MISO */
					pins = "gpio53", "gpio54";
					function = "blsp_spi10";
				};

				config {
					pins = "gpio53", "gpio54";
					drive-strength = <2>; /* 2 MA */
					bias-disable = <0>;
				};
			};

			spi_10_sleep: spi10_sleep {
				mux {
					/* MOSI, MISO */
					pins = "gpio53", "gpio54";
					function = "blsp_spi10";
				};

				config {
					pins = "gpio53", "gpio54";
					drive-strength = <2>; /* 2 MA */
					bias-pull-down;
				};
			};

			spi_10_cs10_active: spi_cs10_active {
				/* CS10 , CLK */
				mux {
					/* MOSI, MISO */
					pins = "gpio55", "gpio56";
					function = "blsp_spi10";
				};

				config {
					pins = "gpio55", "gpio56";
					drive-strength = <2>; /* 2 MA */
					bias-disable = <0>;
				};
			};

			spi_10_cs10_sleep: spi_cs10_sleep {
				mux {
					/* MOSI, MISO */
					pins = "gpio55", "gpio56";
					function = "blsp_spi10";
				};

				config {
					pins = "gpio55", "gpio56";
					drive-strength = <2>; /* 2 MA */
					bias-disable = <0>;
				};

		};

	/* spi_12 */
		spi12 {
			spi_12_active: spi12_active {
				mux {
					/* MOSI, MISO */
					pins = "gpio85", "gpio86";
					function = "blsp_spi12";
				};

				config {
					pins = "gpio85", "gpio86";
					drive-strength = <2>; /* 2 MA */
					bias-disable = <0>;
				};
			};
			
			spi_12_sleep: spi12_sleep {
				mux {
					/* MOSI, MISO */
					pins = "gpio85", "gpio86";
					function = "blsp_spi12";
				};

				config {
					pins = "gpio85", "gpio86";
					drive-strength = <2>; /* 2 MA */
					bias-pull-down;
				};
			};

			spi_12_cs_active: cs12_active {
				/* CS12 , CLK */
				mux {
					pins = "gpio87", "gpio88";
					function = "blsp_spi12";
				};

				config {
					pins = "gpio87", "gpio88";
					drive-strength = <8>; /* 2 MA */
					bias-disable = <0>;
				};
			};

			spi_12_cs_sleep: cs12_sleep {
				mux {
					/* MOSI, MISO */
					pins = "gpio87", "gpio88";
					function = "blsp_spi12";
				};

				config {
					pins = "gpio87", "gpio88";
					drive-strength = <2>; /* 2 MA */
					bias-disable = <0>;
				};
			};
		};
		pmx_i2c_1 {
			i2c_1_active: i2c_1_active {
				mux {
					/* SDA, SCL */
					pins = "gpio2", "gpio3";
					function = "blsp_i2c1";
				};

				config {
					pins = "gpio2", "gpio3";
					drive-strength = <2>; /* 2 MA */
					bias-disable;
				};
			};

			i2c_1_sleep: i2c_1_sleep {
				mux {
					/* SDA, SCL */
					pins = "gpio2", "gpio3";
					function = "blsp_i2c1";
				};

				config {
					pins = "gpio2", "gpio3";
					drive-strength = <2>; /* 2 MA */
					bias-disable;
				};
			};
		};

		audio_ext_devices {
			/* default state */
			audio_ext_speakers: audio_ext_speakers {
				mux {
					pins = "gpio96";
					function = "gpio";
				};

				config {
					pins = "gpio96";
					drive-strength = <2>; /* 2 MA */
					bias-pull-up;
				};
			};
		};

		/* SDC pin type */
		sdc: sdc {
			/*
			 * 0-3 for sdc1
			 * 4-6 for sdc2
			 * 7-9 for sdc3
			 */
			qcom,num-pins = <10>;
			/*
			 * Order of pins:
			 * SDC1: CLK -> 0, CMD -> 1, DATA -> 2, RCLK -> 3
			 * SDC2: CLK -> 4, CMD -> 5, DATA -> 6
			 * SDC3: CLK -> 7, CMD -> 8, DATA -> 9
			 */
			#qcom,pin-cells = <1>;
		};

		pmx_sdc1_clk {
			sdc1_clk_on: clk_on {
				config {
					pins = "sdc1_clk";
					bias-disable;		/* NO pull */
					drive-strength = <16>;	/* 16 MA */
				};
			};
			sdc1_clk_off: clk_off {
				config {
					pins = "sdc1_clk";
					bias-disable;		/* NO pull */
					drive-strength = <2>;	/* 2 MA */
				};
			};
		};

		pmx_sdc1_cmd {
			sdc1_cmd_on: cmd_on {
				config {
					pins = "sdc1_cmd";
					bias-pull-up;		/* pull up */
					drive-strength = <8>;	/* 8 MA */
				};
			};
			sdc1_cmd_off: cmd_off {
				config {
					pins = "sdc1_cmd";
					num-grp-pins = <1>;
					bias-pull-up;		/* pull up */
					drive-strength = <2>;	/* 2 MA */
				};
			};
		};

		pmx_sdc1_data {
			sdc1_data_on: data_on {
				config {
					pins = "sdc1_data";
					bias-pull-up;		/* pull up */
					drive-strength = <8>;	/* 8 MA */
				};
			};
			sdc1_data_off: data_off {
				config {
					pins = "sdc1_data";
					bias-pull-up;		/* pull up */
					drive-strength = <2>;	/* 2 MA */
				};
			};
		};

		pmx_sdc1_rclk {
			sdc1_rclk_on: rclk_on {
				config {
					pins = "sdc1_rclk";
					bias-pull-down;		/* pull down */
				};
			};
			sdc1_rclk_off: rclk_off {
				config {
					pins = "sdc1_rclk";
					bias-pull-down;		/* pull down */
				};
			};
		};

		pmx_sdc2_clk {
			sdc2_clk_on: clk_on {
				config {
					pins = "sdc2_clk";
					bias-disable;		/* NO pull */
					drive-strength = <16>;	/* 16 MA */
