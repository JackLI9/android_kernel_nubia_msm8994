		pmx_sdc2_cmd {
			sdc2_cmd_on: cmd_on {
				config {
					pins = "sdc2_cmd";
					bias-pull-up;		/* pull up */
					drive-strength = <10>;	/* 10 MA */
				};
			};
			sdc2_cmd_off: cmd_off {
				config {
					pins = "sdc2_cmd";
					bias-pull-up;		/* pull up */
					drive-strength = <2>;	/* 2 MA */
				};
			};
		};

		pmx_sdc2_data {
			sdc2_data_on: data_on {
				config {
					pins = "sdc2_data";
					bias-pull-up;		/* pull up */
					drive-strength = <10>;	/* 10 MA */
				};
			};
			sdc2_data_off: data_off {
				config {
					pins = "sdc2_data";
					bias-pull-up;		/* pull up */
					drive-strength = <2>;	/* 2 MA */
				};
			};
		};

		pmx_sdc3_clk {
			sdc3_clk_on: clk_on {
				config {
