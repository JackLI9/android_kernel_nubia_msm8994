		pmx_i2c_6 {
			/* active state */
			i2c_6_active: i2c_6_active{
				mux {
					/* SDA, SCL */
					pins = "gpio28", "gpio27";
					function = "blsp_i2c6";
				};

				config {
					pins = "gpio28", "gpio27";
					drive-strength = <2>; /* 2 MA */
